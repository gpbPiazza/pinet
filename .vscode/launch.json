{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch pihttp example server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/pihttp/example/server/main.go",
    },
    {
      "name": "Launch pihttp example client",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/pihttp/example/client/main.go",
      "args": [
        "http"
      ]
    },
    {
      "name": "Launch httpfromtcp",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/httpfromtcp/main.go"
    },
  ]
}